<div class="page-content">
	<div class="contact-header">
		<div class="item-photo">
			<img src="img/cats/{{model.picId}}.png">
		</div>
		<div class="header-text">
			<h3>{{model.firstName}} {{model.lastName}}</h3>
			<p>{{model.company}}</p>
		</div>
	</div>
	<div class="list-block">
		<div class="list-group">
			<ul>
				{{#if model.phone}}
					<li>
						<a href="tel:{{model.phone}}" class="item-link item-content external">
							<div class="item-media"><i class="icon ion-ios7-telephone-outline"></i></div>
							<div class="item-inner">
								<div class="item-title">{{model.phone}}</div>
							</div>
						</a>
					</li>

				{{/if}}
				{{#if model.email}}
					<li>
					<div style="max-width:900px;">
					<div style="position:relative;padding-bottom:56.25%;padding-top:25px;">
					<iframe allowfullscreen="true" frameborder="0" src="{{model.email}}" style="position:absolute; top:0; left:0; width:100%; height:100%;" webkit=""></iframe>
					</div>
					</div>
					</li>
				{{/if}}
				{{#if model.city}}
					<li class="item-content">
						<div class="item-media"><i class="icon ion-ios7-eye-outline"></i></div>
						<div class="item-inner">
							<div class="item-title">{{model.city}}</div>
						</div>
					</li>
				{{/if}}
			</ul>
		</div>
	</div>
</div>



