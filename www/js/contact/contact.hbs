<div class="page-content">
	<div class="contact-header">
		<div class="header-text">
			<p>{{model.firstName}}</p>
		</div>
	</div>
	<div class="list-block">
		<div class="list-group">
			<ul>
				{{#if model.email}}
					<li>
					<div style="max-width:900px;">
					<div style="position:relative;padding-bottom:120%;padding-top:25px;">
					<iframe allowfullscreen="true" frameborder="0" src="{{model.email}}" style="position:absolute; top:0; left:0; width:100%; height:100%;" webkit=""></iframe>
					</div>
					</div>
					</li>
				{{/if}}
			</ul>
		</div>
	</div>
</div>



